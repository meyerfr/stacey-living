<%= render 'shared/navbar' %>
<h1 style="padding-top: 50px">This is the Page where all our Projects are shown</h1>
<% @flats.each do |flat| %>
  <%= link_to flat_rooms_path(flat)  do %>
    <h1>Project <%= flat.project_name %></h1>
    <%= link_to 'Edit', edit_flat_path(flat) %>
    <%= link_to 'Delete', flat_path(flat), method: :delete %>
  <% end %>
<% end %>
<br>
<%= link_to 'Create new Flat', new_flat_path, class: "stacey-button" %>
