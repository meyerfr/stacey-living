<div class="three-steps-navbar">
  <div class="go-back-button">
    <p>< Go Back</p>
  </div>
  <div class="three-steps-container">
    <div class="ring active">
      <p>1</p>
    </div>
    <div class="line"></div>
    <div class="ring">
      <p>2</p>
    </div>
    <div class="line"></div>
    <div class="ring">
      <p>3</p>
    </div>
  </div>
  <div class="phone">
    <%= link_to 'Call us on 022198819134', "tel: 022198819134" %>
  </div>
</div>
<%= render 'shared/projects_banner', flat: @flat %>
<div class="room-cards-list-wrapper">
  <div class="room-card-wrapper">
    <% @rooms.each do |room| %>
      <%= render 'shared/room_cards', flat: @flat, room: room, link: "/flats/#{@flat.id}/rooms/#{room.id}/user/#{@user.id}/#{@authentity_token_contract}/room_detail_show" %>
    <% end %>
    <%#= render 'shared/rooms_detail_popup', room: Room.first %>
  </div>
</div>
<div class="contract-page-quote">
  <p>”ALONE WE CAN DO SO LITTLE;</p>
  <p style="margin-bottom: 25px;">TOGETHER WE CAN DO SO MUCH”</p>
  <p class="quote-name">Helen Keller</p>
</div>
<%#= render 'shared/community_area' %>
<%= render 'shared/contract_footer', flat: @flat %>
