<div class="users-card-wrapper">
  <div class="users-card-container">
    <div class="users-card-img" style="background-image: linear-gradient(220deg, rgba(0,0,0,0) 65%, rgba(0,0,0,0.8) 90%), url('<%= user.photo.url(:bright_face) %>');">
      <div class="users-card-name">
        <h3 class="users-card-name-light"><%= user.first_name %></h3>
        <h3><%= user.last_name %></h3>
      </div>
    </div>
    <div class="users-card-content">
      <span>
        <span class="city">Hamburg</span>
        <span> - </span>
        <span>Apr '19</span>
      </span>
    </div>
    <a class="users-card-link" href="/users/<%= user.id %>"></a>
  </div>
</div>
